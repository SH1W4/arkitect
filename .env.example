# Configurações do Ambiente
ENVIRONMENT=development
LOG_LEVEL=INFO
DEBUG=false

# Redis
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# EON Framework
EON_FRAMEWORK_HOST=localhost
EON_FRAMEWORK_PORT=8001
EON_FRAMEWORK_TIMEOUT=30
EON_FRAMEWORK_API_KEY=

# Task Mesh
TASK_MESH_WORKERS=4
TASK_MESH_QUEUE_SIZE=1000
TASK_MESH_RETRY_LIMIT=3

# Monitoramento
ENABLE_METRICS=true
METRICS_PORT=9090

